{
  "name": "angularstarter",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "prestart": "npm run tailwind",
    "start": "ng serve",
    "prebuild": "npm run tailwind",
    "build": "ng build --prod",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "tailwind": "tailwind build tailwind-build.scss -c ./tailwind.js -o ./src/tailwind.scss"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.0",
    "@angular/cdk": "^6.0.1",
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/material": "^6.0.1",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/router": "^6.0.0",
    "@ngrx/effects": "^5.2.0",
    "@ngrx/router-store": "^5.2.0",
    "@ngrx/store": "5.2.0",
    "@ngrx/store-devtools": "5.2.0",
    "core-js": "^2.5.6",
    "install": "^0.11.0",
    "jwt-decode": "^2.2.0",
    "npm": "^6.0.0",
    "rxjs": "^6.1.0",
    "rxjs-compat": "^6.1.0",
    "rxjs-marbles": "^3.0.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "6.0.0",
    "@angular/compiler-cli": "^6.0.0",
    "@angular/language-service": "^6.0.0",
    "@types/jest": "^22.2.3",
    "@types/node": "~10.0.4",
    "codelyzer": "^4.3.0",
    "jest": "^22.4.3",
    "jest-cli": "^22.4.3",
    "jest-html-reporter": "^2.3.0",
    "jest-preset-angular": "^5.2.2",
    "ngrx-store-freeze": "^0.2.2",
    "ngrx-store-localstorage": "^5.0.0",
    "protractor": "~5.3.1",
    "tailwindcss": "^0.5.2",
    "ts-node": "~6.0.3",
    "tslint": "~5.10.0",
    "typescript": "2.7.2",
    "@angular-devkit/build-angular": "~0.6.0"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "testResultsProcessor": "./node_modules/jest-html-reporter",
    "setupTestFrameworkScriptFile": "<rootDir>/src/jestSetup.ts",
    "globals": {
      "__TS_CONFIG__": "src/tsconfig.spec.json",
      "__TRANSFORM_HTML__": true
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/src/jest*.ts"
    ],
    "transform": {
      "^.+\\.(ts|js|html)$": "<rootDir>/node_modules/jest-preset-angular/preprocessor.js"
    },
    "moduleFileExtensions": [
      "ts",
      "js",
      "html"
    ],
    "moduleNameMapper": {},
    "transformIgnorePatterns": [
      "node_modules/(?!@ngrx)"
    ],
    "modulePathIgnorePatterns": [
      "main.spec.ts"
    ]
  }
}
